<div class="row">
    <div class="col-md-12">
        
        <h3>{{session.user.userName}} ( {{session.user.email}} ) History</h3><hr>
        <div class="abc">
            <table>
                <tr>
                    <th>Login Date/Time</th>
                    <th>Client Information</th>
                </tr>
                    {{#each session.user.loginHistory}}     
                    {{!-- #each will iterate through all times when user logged in --}}
                    <tr>
                        <td>{{  dateTime    }}</td>
                        <td>{{  userAgent   }}</td>
                    </tr>
                    {{/each}}
            </table>
        </div>
    </div>
</div>